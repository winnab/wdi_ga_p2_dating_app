.container
  %h1 Explore

  #profile-form
    = render "form"
  %br

  #div
    - @users.each do |user|
      = link_to user.first_name, user_path(user)
      %br
      = image_tag user.avatar.search
      %br
      = user.username
      \,
      = user.age
      \,
      = user.location
      %br

  = will_paginate @users

  %p
    Why don't you...
    = link_to "Sign up", new_user_registration_path
    ?
